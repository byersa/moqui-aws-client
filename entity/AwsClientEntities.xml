<entities xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://moqui.org/xsd/entity-definition-1.2.xsd">


    <entity entity-name="AwsCredential" package-name="moqui.awsclient" >
        <description>Contains keys for attaching to AWS services</description>
        <field name="keyId" type="id" is-pk="true"><description></description></field> 
        <field name="accessKey" type="id-long"><description>access key</description></field>
        <field name="secretAccessKey" type="id-long"><description>secret access key</description></field>
    </entity>

</entities>
